<svg viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f1f2e;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#162a3f;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f1f2e;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="mvpGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#28a745;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#28a745;stop-opacity:0.05" />
    </linearGradient>
    
    <linearGradient id="fullGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8b5cf6;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:0.05" />
    </linearGradient>
    
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bgGradient)"/>
  
  <!-- Title -->
  <text x="600" y="80" text-anchor="middle" fill="#ffffff" font-family="Poppins, sans-serif" font-size="36" font-weight="bold">
    MVP vs Producto Completo
  </text>
  <text x="600" y="120" text-anchor="middle" fill="#a0aec0" font-family="Poppins, sans-serif" font-size="18">
    La Estrategia que Realmente Funciona
  </text>
  
  <!-- Left Side - MVP -->
  <g transform="translate(50, 160)">
    <!-- MVP Container -->
    <rect x="0" y="0" width="500" height="400" rx="20" fill="url(#mvpGradient)" stroke="#28a745" stroke-width="2"/>
    
    <!-- MVP Title -->
    <text x="250" y="40" text-anchor="middle" fill="#28a745" font-family="Poppins, sans-serif" font-size="28" font-weight="bold">MVP</text>
    <text x="250" y="65" text-anchor="middle" fill="#ffffff" font-family="Poppins, sans-serif" font-size="16">Minimum Viable Product</text>
    
    <!-- MVP Rocket Icon -->
    <g transform="translate(200, 90)">
      <path d="M50 0 L60 20 L100 15 L70 35 L80 55 L50 45 L20 55 L30 35 L0 15 L40 20 Z" fill="#28a745" filter="url(#glow)"/>
      <circle cx="50" cy="25" r="8" fill="#ffffff"/>
    </g>
    
    <!-- MVP Features -->
    <g transform="translate(30, 180)">
      <circle cx="10" cy="10" r="5" fill="#28a745"/>
      <text x="25" y="15" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Desarrollo rápido (2-8 semanas)</text>
      
      <circle cx="10" cy="40" r="5" fill="#28a745"/>
      <text x="25" y="45" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Funcionalidad core únicamente</text>
      
      <circle cx="10" cy="70" r="5" fill="#28a745"/>
      <text x="25" y="75" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Feedback rápido del mercado</text>
      
      <circle cx="10" cy="100" r="5" fill="#28a745"/>
      <text x="25" y="105" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Inversión mínima inicial</text>
      
      <circle cx="10" cy="130" r="5" fill="#28a745"/>
      <text x="25" y="135" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Validación de hipótesis</text>
      
      <circle cx="10" cy="160" r="5" fill="#28a745"/>
      <text x="25" y="165" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Iteración basada en datos</text>
    </g>
  </g>
  
  <!-- Right Side - Producto Completo -->
  <g transform="translate(650, 160)">
    <!-- Full Product Container -->
    <rect x="0" y="0" width="500" height="400" rx="20" fill="url(#fullGradient)" stroke="#8b5cf6" stroke-width="2"/>
    
    <!-- Full Product Title -->
    <text x="250" y="40" text-anchor="middle" fill="#8b5cf6" font-family="Poppins, sans-serif" font-size="28" font-weight="bold">PRODUCTO COMPLETO</text>
    <text x="250" y="65" text-anchor="middle" fill="#ffffff" font-family="Poppins, sans-serif" font-size="16">Solución Integral</text>
    
    <!-- Full Product Castle Icon -->
    <g transform="translate(200, 90)">
      <rect x="10" y="30" width="80" height="40" fill="#8b5cf6" filter="url(#glow)"/>
      <rect x="0" y="20" width="20" height="20" fill="#8b5cf6"/>
      <rect x="30" y="10" width="20" height="30" fill="#8b5cf6"/>
      <rect x="50" y="10" width="20" height="30" fill="#8b5cf6"/>
      <rect x="80" y="20" width="20" height="20" fill="#8b5cf6"/>
      <rect x="40" y="45" width="20" height="25" fill="#ffffff"/>
    </g>
    
    <!-- Full Product Features -->
    <g transform="translate(30, 180)">
      <circle cx="10" cy="10" r="5" fill="#8b5cf6"/>
      <text x="25" y="15" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Desarrollo extendido (3-6 meses)</text>
      
      <circle cx="10" cy="40" r="5" fill="#8b5cf6"/>
      <text x="25" y="45" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Todas las funcionalidades</text>
      
      <circle cx="10" cy="70" r="5" fill="#8b5cf6"/>
      <text x="25" y="75" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Experiencia completa desde día 1</text>
      
      <circle cx="10" cy="100" r="5" fill="#8b5cf6"/>
      <text x="25" y="105" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Mayor inversión inicial</text>
      
      <circle cx="10" cy="130" r="5" fill="#8b5cf6"/>
      <text x="25" y="135" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Menor riesgo de mercado</text>
      
      <circle cx="10" cy="160" r="5" fill="#8b5cf6"/>
      <text x="25" y="165" fill="#ffffff" font-family="Poppins, sans-serif" font-size="14">Diferenciación competitiva</text>
    </g>
  </g>
  
  <!-- VS Separator -->
  <circle cx="600" cy="360" r="40" fill="#162a3f" stroke="#28a745" stroke-width="3"/>
  <text x="600" y="370" text-anchor="middle" fill="#28a745" font-family="Poppins, sans-serif" font-size="20" font-weight="bold">VS</text>
  
  <!-- Bottom Call to Action -->
  <text x="600" y="610" text-anchor="middle" fill="#a0aec0" font-family="Poppins, sans-serif" font-size="16">
    ¿Cuál estrategia es la correcta para tu proyecto?
  </text>
</svg> 